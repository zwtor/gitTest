<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="酷学院考试-试卷管理" thread-count="2">
	<groups>
		<run>
			<include name="CI"/>
		</run>
	</groups>
	<test name="试卷管理模块流程测试">
		<classes>
			<class name="exam.cases.TestPaperCases" />
		</classes>
	</test>
	 <test name="试卷导出">
		<classes>
			<class name="exam.cases.TestExportBlankPaper" />
		</classes>
	</test>
	<test name="试卷复制">
		<classes>
			<class name="exam.cases.TestCopyPaper" />
		</classes>
	</test>
	<test name="设置试卷的可见范围">
		<classes>
			<class name="exam.cases.TestSetPaperVisible" />
		</classes>
	</test>
	<test name="新考试试卷测试">
		<classes>
			<class name="exam.cases.TestNewExamPaper" />
		</classes>
	</test>
	
</suite>